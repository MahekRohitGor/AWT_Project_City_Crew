{
    "version": 2,
    "builds": [
      {
        "src": "operators/package.json",
        "use": "@vercel/static-build",
        "config": { "distDir": "operators/dist" }
      },
      {
        "src": "backend/package.json",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/backend/src/index.js"
      },
      {
        "src": "/(.*)",
        "dest": "/operators/dist/$1"
      }
    ]
  }
  