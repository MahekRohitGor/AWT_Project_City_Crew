{
    "version": 2,
    "builds": [
      {
        "src": "operators/package.json",
        "use": "@vercel/static-build",
        "config": { "distDir": "operators/dist" }
      },
      {
        "src": "backend/index.js",
        "use": "@vercel/node"
      },
      {
        "src": "backend/server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/backend/(.*)", 
        "dest": "/backend/$1"
      },
      {
        "src": "/(.*)", 
        "dest": "/operators/dist/$1"
      }
    ]
  }
  